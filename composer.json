{
  "name": "shankar/laravel-firebase-eloquent",
  "description": "Lightweight Eloquent-like models for Firebase Database.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/pagination": "^10.0|^11.0|^12.0",
    "kreait/laravel-firebase": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Shankar\\FirebaseDb\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shankar\\FirebaseDb\\FirebaseDbServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}